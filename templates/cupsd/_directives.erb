LogLevel warn
PageLogFormat
MaxLogSize 0
<% unless @listen.empty? -%>
<%   [@listen].flatten.each do |l| -%>
<%=    "Listen #{l}" %>
<%   end -%>
<% end -%>
<%= '# ' if @browse_dnssd_subtypes.nil? -%>BrowseDNSSDSubTypes_<%= @browse_dnssd_subtypes.is_a?(Array) ? @browse_dnssd_subtypes.join(',_') : @browse_dnssd_subtypes %>
<%= '# ' if @browse_local_protocols.nil? -%>BrowseLocalProtocols <%= @browse_local_protocols.is_a?(Array) ? @browse_local_protocols.join(' ') : @browse_local_protocols %>
<%= '# ' if @browse_web_if.nil? -%>BrowseWebIF <%= (@browse_web_if ? 'Yes' : 'No') %>
<%= '# ' if @browsing.nil? -%>Browsing <%= (@browsing ? 'Yes' : 'No') %>
DefaultAuthType Basic
<%= '# ' if @server_alias.nil? -%>ServerAlias <%= @server_alias.is_a?(Array) ? @server_alias.join(' ') : @server_alias %>
<%= '# ' if @server_name.nil? -%>ServerName <%= @server_name %>
<%= '# ' if @web_interface.nil? -%>WebInterface <%= (@web_interface ? 'Yes' : 'No') %>
